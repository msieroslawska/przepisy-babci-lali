{
  "include": ["./src/**/*", "./gatsby-config.ts", "./gatsby-node.ts"],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
      "jsx": "react",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}